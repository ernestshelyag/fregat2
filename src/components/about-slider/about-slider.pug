    var aboutSliderInfo = [
        {"title": "2016 — 2019","text": "Даты строительства"},
        {"title": "25","text": "Этажей"},
        {"title": "110","text": "Тыс. м² жилья"},
        {"title": "5","text": "Спортплощадок"},

+b.SECTION.about-slider
    +e.wrp.wrp
        +e.title-block
            +e.H2.title О проекте Фрегат 2
            +e.arrs
        +e.cont
            +e.info-block
                +e.UL.list
                    each item in aboutSliderInfo
                        +e.LI.item
                            h3= item.title
                            p= item.text
                            span= item.title
            +e.slider-block
                +e.slider
                    +e.FIGURE.slide(style="background-image: url(img/house-slider.jpg);")
                        h3 Дом 1А
                        p Сдан в 2007
                    +e.FIGURE.slide(style="background-image: url(img/house-slider.jpg);")
                        h3 Дом 2C
                        p Сдан в 2009
